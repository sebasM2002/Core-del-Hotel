@model IEnumerable<CoreHotel.Models.ReservacionesModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id_Huesped)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Id_Habitacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.check_In)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fecha_Entrada)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fecha_Salida)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cantidad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.is_Deleted)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Created_at)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Updated_at)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id_Huesped)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Id_Habitacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.check_In)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha_Entrada)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha_Salida)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cantidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.is_Deleted)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Created_at)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Updated_at)
            </td>
            <td>
                <a asp-action="Delete" asp-route-id="@item.Id_Reservacion">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
